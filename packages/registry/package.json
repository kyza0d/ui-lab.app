{
  "name": "ui-lab-registry",
  "version": "0.1.6",
  "description": "Component registry and metadata management for ui-lab-components",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "react",
    "components",
    "registry",
    "metadata",
    "ui-lab"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kyza0d/ui-lab.app"
  },
  "homepage": "https://github.com/kyza0d/ui-lab.app#readme",
  "bugs": {
    "url": "https://github.com/kyza0d/ui-lab.app/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./generated-data": {
      "import": "./dist/generated-data.js",
      "types": "./dist/generated-data.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    },
    "./components/*/examples": {
      "import": "./dist/components/*/examples/index.js",
      "types": "./dist/components/*/examples/index.d.ts"
    },
    "./components/*/examples.json": {
      "import": "./dist/components/*/examples.json"
    },
    "./elements": {
      "import": "./dist/elements/index.js",
      "types": "./dist/elements/index.d.ts"
    },
    "./elements/*": {
      "import": "./dist/elements/*/index.js",
      "types": "./dist/elements/*/index.d.ts"
    },
    "./elements/*/variations/*": {
      "import": "./dist/elements/*/variations/*/index.js",
      "types": "./dist/elements/*/variations/*/index.d.ts"
    },
    "./elements/*/manifest": {
      "import": "./dist/elements/*/manifest.js",
      "types": "./dist/elements/*/manifest.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsx scripts/generate-examples-json.ts && tsx scripts/generate-registry-from-components.ts && tsx scripts/generate-registry-data.ts && node scripts/generate-variations.js && node scripts/generate-manifest.js && tsc",
    "type-check": "tsc --noEmit",
    "generate": "tsx scripts/generate-examples-json.ts && tsx scripts/generate-registry-from-components.ts && tsx scripts/generate-registry-data.ts && node scripts/generate-variations.js && node scripts/generate-manifest.js",
    "validate": "tsx scripts/validate-registry.ts"
  },
  "dependencies": {
    "react-icons": "^5.5.0",
    "ui-lab-components": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "react": "^19",
    "react-docgen-typescript": "^2.2.2",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
