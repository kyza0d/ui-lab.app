{
  "name": "ui-lab-registry",
  "version": "0.1.7",
  "description": "Component registry and metadata management for ui-lab-components",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "react",
    "components",
    "registry",
    "metadata",
    "ui-lab"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kyza0d/ui-lab.app"
  },
  "homepage": "https://github.com/kyza0d/ui-lab.app#readme",
  "bugs": {
    "url": "https://github.com/kyza0d/ui-lab.app/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./generated-data": {
      "import": "./dist/generated-data.js",
      "types": "./dist/generated-data.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    },
    "./components/*/examples": {
      "import": "./dist/components/*/examples/index.js",
      "types": "./dist/components/*/examples/index.d.ts"
    },
    "./components/*/examples.json": {
      "import": "./dist/components/*/examples.json"
    },
    "./elements": {
      "import": "./dist/elements/index.js",
      "types": "./dist/elements/index.d.ts"
    },
    "./elements/*": {
      "import": "./dist/elements/*/index.js",
      "types": "./dist/elements/*/index.d.ts"
    },
    "./elements/*/variations/*": {
      "import": "./dist/elements/*/variations/*/index.js",
      "types": "./dist/elements/*/variations/*/index.d.ts"
    },
    "./elements/*/manifest": {
      "import": "./dist/elements/*/manifest.js",
      "types": "./dist/elements/*/manifest.d.ts"
    },
    "./sections": {
      "import": "./dist/sections/index.js",
      "types": "./dist/sections/index.d.ts"
    },
    "./sections/*": {
      "import": "./dist/sections/*/index.js",
      "types": "./dist/sections/*/index.d.ts"
    },
    "./sections/*/variations/*": {
      "import": "./dist/sections/*/variations/*/index.js",
      "types": "./dist/sections/*/variations/*/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm validate:elements && pnpm generate && npm run build:css && tsc",
    "build:css": "pnpm exec postcss src/styles.css -o dist/styles.css",
    "type-check": "tsc --noEmit",
    "generate": "tsx scripts/generate-examples-json.ts && tsx scripts/generate-registry-from-components.ts && tsx scripts/generate-registry-data.ts && tsx scripts/generate-element-variations.ts && tsx scripts/generate-element-registry.ts && tsx scripts/generate-demo-map.ts && tsx scripts/generate-manifest.ts && tsx scripts/validate-element-structure.ts",
    "validate": "tsx scripts/validate-registry.ts",
    "validate:elements": "tsx scripts/validate-element-registry.ts"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "react-icons": "^5.5.0",
    "ui-lab-components": "workspace:*"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "chalk": "^5.6.2",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.0",
    "postcss-load-config": "^6.0.1",
    "react": "^19",
    "react-docgen-typescript": "^2.2.2",
    "tailwindcss": "^4.1.11",
    "ts-morph": "^27.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
