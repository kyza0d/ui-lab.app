{
  "name": "ui-lab",
  "version": "0.1.0",
  "description": "CLI for installing ui-lab components",
  "type": "module",
  "bin": {
    "ui-lab": "./dist/bin/ui-lab.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "commander": "^12.1.0",
    "picocolors": "^1.1.1",
    "ui-lab-registry": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "ui-lab",
    "cli",
    "components",
    "tailwind"
  ],
  "license": "MIT"
}
