{
  "name": "ui-lab-mcp",
  "version": "0.1.0",
  "description": "MCP server for UI Lab component library - enables AI agents to discover, understand, and generate production-ready code",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ui-lab-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
