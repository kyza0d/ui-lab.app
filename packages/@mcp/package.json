  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "ui-lab-registry": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
