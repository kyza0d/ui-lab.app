{
  "name": "ui-lab-components",
  "version": "0.1.01",
  "description": "A collection of beautiful, accessible React components built with Radix UI and Tailwind CSS",
  "private": false,
  "type": "module",
  "license": "MIT",
  "main": "./dist/ui-lab-ui.umd.js",
  "module": "./dist/ui-lab-ui.es.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "react",
    "components",
    "ui",
    "radix-ui",
    "tailwindcss",
    "accessibility",
    "a11y"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kyza0d/ui-lab.app"
  },
  "homepage": "https://github.com/kyza0d/ui-lab.app#readme",
  "bugs": {
    "url": "https://github.com/kyza0d/ui-lab.app/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/ui-lab-ui.es.js",
      "require": "./dist/ui-lab-ui.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && npm run build:css",
    "build:css": "npx postcss src/styles.css -o dist/styles.css && cat dist/style.css >> dist/styles.css",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.14.12",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.1",
    "postcss-cli": "^11.0.0",
    "postcss-load-config": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "typescript": "^5",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^3.9.1"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^1.1.1",
    "gsap": "^3.12.2",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react-icons": "^5.5.0"
  }
}
