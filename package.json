{
  "name": "ui-lab-monorepo",
  "version": "0.1.0",
  "private": true,
  "description": "UI Lab - A modern component library",
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm --filter ./apps/* run dev",
    "dev:site": "pnpm --filter @ui-lab/site run dev",
    "build": "pnpm -r run build",
    "build:packages": "pnpm --filter './packages/*' run build",
    "build:site": "pnpm --filter @ui-lab/site run build",
    "build:push": "pnpm run build:packages && yalc push",
    "type-check": "pnpm -r run type-check",
    "lint": "pnpm -r run lint",
    "clean": "pnpm -r run clean",
    "publish": "pnpm publish -r",
    "use:local": "node scripts/switch-packages.mjs local",
    "use:prod": "node scripts/switch-packages.mjs prod",
    "use:status": "node scripts/switch-packages.mjs status"
  },
  "dependencies": {
    "next": "^16.0.10",
    "ui-lab-registry": "^0.1.0"
  },
  "devDependencies": {
    "typescript": "^5"
  }
}
